<suite name="Group of group Suite" verbose="1">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>

	<test name="Group of group Test">
		<groups>
			<define name="include-group">
				<include name="entities-tests" />
			</define>
			<define name="exclude-group">
				<!-- <include name="test-to-exclude" /> -->
			</define>
			<run>
				<include name="include-group" />
				<exclude name="exclude-group" />
			</run>
		</groups>
		<classes>
			<class name="com.almundo.automation.tests.EntitiesTests" />
			<!-- <methods>
				<include name="verifyChoiceEntity"
					invocation-numbers="0" />
			</methods>
			 -->
		</classes>
	</test>
</suite>